<resources xmlns:xliff="http://schemas.android.com/apk/res-auto">
  <string name="app_name">Sunshine</string>
  <string name="hello_world">Hello world!</string>
  <string name="action_settings">Settings</string>
  <string name="action_refresh">Refresh</string>
  <string name="action_map">Map Location</string>
  <string name="action_share">Share</string>

  <!-- Settings Activity Title-->
  <string name="title_activity_settings">Settings</string>

  <!-- Label for location preference [CHAT LIMIT=30] -->
  <string name="pref_location_label">Location</string>

  <!-- Key name for storing location in SharedPreferences [CHAR LIMIT=NONE] -->
  <string name="pref_location_key" translatable="false">location</string>

  <!--  Default postal code for location preference [CHAR LIMIT=NONE] -->
  <string name="pref_location_default" translatable="false">94043</string>

  <string name="pref_units_label">Temperature</string>
  <string name="pref_units_key">temperature</string>
  <string name="pref_units_celsius">celsius</string>
  <string name="pref_units_farenheit">farenheit</string>
  <string name="today">Today</string>
  <string name="tomorrow">Tomorrow</string>

  <!-- Date format for displaying day of week and date (i.e. Mon Jun 1) -->
  <string name="format_full_friendly_date">
    <xliff:g id="day">%1$s</xliff:g>, <xliff:g id="date">%2$s</xliff:g>
  </string>
  
  <string name="format_temperature">
    <xliff:g id="temp">%1.0f</xliff:g>
    \u00B0
  </string>

  <!-- wind speed -->
  <string name="format_wind_mph"><xliff:g id="wind_speed">%1.0f</xliff:g> mph <xliff:g id="wind_dir">%s</xliff:g></string>
  <string name="format_wind_kmh"><xliff:g id="wind_speed">%1.0f</xliff:g> km/h <xliff:g id="wind_dir">%s</xliff:g></string>

  <!-- pressure -->
  <string name="format_pressure"><xliff:g id="pressure">%1.0f</xliff:g> hPa</string>

  <!-- Humidity-->
  <string name="format_humidity"><xliff:g id="humidity">%d</xliff:g> %%</string>

  <!-- Empty Weather Database -->
  <string name="empty_forecast_list">No Weather Information Available</string>
  <string name="empty_forecast_list_no_network">No Weather Information Available. The network is not available to fetch weather data.</string>

  <string name="format_art_pack_url">https://raw.githubusercontent.com/udacity/Sunshine-Version-2/sunshine_master/app/src/main/res/drawable-xxhdpi/art_%s.png</string>

  <string name="humidity">Humidity</string>
  <string name="pressure">Pressure</string>
  <string name="wind">Wind</string>

</resources>
